{
    "kind": "template",
    "name": "dashui:ecpRelationshipMap",
    "target": "intersightdevnetpod12:account",
    "view": "default",
    "element": {
        "instanceOf": "leftbar",
        "elements": [
            {
                "key": "account",
                "path": ".",
                "entityAttribute": "intersightdevnetpod12.account.name",
                "iconName": "AgentType.Appd"
            },
            {
                "key": "hyperflex_cluster",
                "path": "out(common:consists_of).to(intersightdevnetpod12:hyperflex_cluster)",
                "entityAttribute": "intersightdevnetpod12.hyperflex.device.id",
                "iconName": "AgentType.Appd"
            },
            {
                "key": "ucs_domain",
                "path": "out(common:consists_of).to(intersightdevnetpod12:ucs_domain)",
                "entityAttribute": "intersightdevnetpod12.host.name",
                "iconName": "AgentType.Appd"
            }
        ]
    }
}