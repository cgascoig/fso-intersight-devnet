{
    "kind": "template",
    "name": "dashui:ecpRelationshipMap",
    "target": "intersightdevnetpod2:ucs_domain",
    "view": "default",
    "element": {
        "instanceOf": "leftbar",
        "elements": [
            {
                "key": "ucs_domain",
                "path": ".",
                "entityAttribute": "intersightdevnetpod2.host.name",
                "iconName": "AgentType.Appd"
            },
            {
                "key": "account",
                "path": "in(common:consists_of).from(intersightdevnetpod2:account)",
                "entityAttribute": "intersightdevnetpod2.account.name",
                "iconName": "AgentType.Appd"
            }
        ]
    }
}