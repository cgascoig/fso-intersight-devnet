{
    "kind": "template",
    "name": "intersightdevnetpod2:accountDetailsList",
    "target": "intersightdevnetpod2:account",
    "view": "default",
    "element": {
        "instanceOf": "html",
        "elements": [
            {
                "instanceOf": "card",
                "props": {
                    "title": "VM Count"
                },
                "elements": [
                    {
                        "instanceOf": "cartesian",
                        "props": {
                            "style": {
                                "height": 250
                            }
                        },
                        "children": [
                            {
                                "props": {
                                    "name": "intersightdevnetpod2:intersightdevnetpod2.vm_count"
                                },
                                "metric": {
                                    "name": "intersightdevnetpod2:intersightdevnetpod2.vm_count",
                                    "y": {
                                        "type": "value"
                                    }
                                },
                                "type": "LINE"
                            }
                        ]
                    }
                ]
            }
        ],
        "style": {
            "display": "flex",
            "flexDirection": "column",
            "gap": 12
        }
    }
}