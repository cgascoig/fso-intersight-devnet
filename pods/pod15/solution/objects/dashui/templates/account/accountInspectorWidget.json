{
    "kind": "template",
    "name": "intersightdevnetpod15:accountInspectorWidget",
    "target": "intersightdevnetpod15:account",
    "view": "default",
    "element": {
        "instanceOf": "elements",
        "elements": [
            {
                "instanceOf": {
                    "name": "inspectorWidget"
                },
                "elements": {
                    "instanceOf": {
                        "name": "properties"
                    },
                    "elements": [
                        {
                            "label": {
                                "instanceOf": "text",
                                "content": "intersightdevnetpod15.account.name"
                            },
                            "value": {
                                "instanceOf": "string",
                                "path": "attributes(intersightdevnetpod15.account.name)"
                            }
                        }
                    ]
                },
                "title": "Properties"
            },
            {
                "elements": [
                    {
                        "additionalInformation": {
                            "branches": [
                                {
                                    "elements": {
                                        "instanceOf": "empty"
                                    },
                                    "value": null
                                }
                            ],
                            "defaultElements": {
                                "elements": [
                                    {
                                        "emptyContent": "0",
                                        "instanceOf": "number",
                                        "path": "metrics(intersightdevnetpod15:intersightdevnetpod15.vm_count, \"intersight-otel\").value"
                                    },
                                    {
                                        "content": " virtual machines",
                                        "instanceOf": "text"
                                    }
                                ],
                                "instanceOf": "elements"
                            },
                            "path": "metrics(intersightdevnetpod15:intersightdevnetpod15.vm_count, \"intersight-otel\").value"
                        },
                        "graph": {
                            "path": "intersightdevnetpod15:intersightdevnetpod15.vm_count",
                            "source": "intersight-otel"
                        },
                        "headline": "Virtual Machine Count",
                        "instanceOf": {
                            "name": "intersightdevnetpod15:sparkgraph"
                        }
                    }
                ],
                "instanceOf": {
                    "name": "inspectorWidget"
                }
            }
        ]
    }
}